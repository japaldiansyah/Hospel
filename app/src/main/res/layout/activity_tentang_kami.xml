<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#91D8E1"
    tools:context=".TentangKami">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="@drawable/bgawan"
        android:layout_alignParentTop="true">
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="250dp"
        android:layout_alignParentBottom="true"
        android:background="@drawable/kota">
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/bar_atas_tentang_kami"
        android:layout_marginTop="25dp"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:layout_width="match_parent"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/back_tentang_kami"
            android:layout_width="wrap_content"
            android:layout_centerVertical="true"
            android:layout_height="wrap_content"
            android:src="@drawable/back_ic" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tentang Kami"
            android:textSize="30sp"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:fontFamily="@font/fredom"/>
    </RelativeLayout>

    <TextView
        android:id="@+id/developer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/bar_atas_tentang_kami"
        android:layout_marginTop="50dp"
        android:text="DEVELOPER"
        android:fontFamily="@font/dm_sans"
        android:textColor="@color/black"
        android:textSize="20sp"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="25dp"
        android:layout_marginEnd="25dp"
        android:background="@drawable/bg_rounded_text"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/developer">

        <RelativeLayout
            android:id="@+id/satu1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true">

            <TextView
                android:id="@+id/nama_1"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Aditya Yuda Pamungkas"
                android:fontFamily="@font/dm_sans"
                android:textColor="@color/black"
                android:layout_centerHorizontal="true"/>
            <RelativeLayout
                android:id="@+id/satu"
                android:layout_width="wrap_content"
                android:layout_below="@id/nama_1"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/gambar_1"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/instagram"/>

                <TextView
                    android:id="@+id/ig_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="adtyyd_p"
                    android:fontFamily="@font/dm_sans"
                    android:textColor="@color/black"
                    android:layout_toRightOf="@id/gambar_1"
                    android:layout_marginTop="10dp"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>

            <View
                android:id="@+id/divider_1"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#000000"
                android:layout_below="@id/satu"
                app:layout_constraintVertical_bias="0.0" />
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/dua2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/satu1">
            <TextView
                android:id="@+id/nama_2"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Lintang Adi Cahya Ramadhani"
                android:fontFamily="@font/dm_sans"
                android:textColor="@color/black"
                android:layout_centerHorizontal="true"/>
            <RelativeLayout
                android:id="@+id/dua"
                android:layout_width="wrap_content"
                android:layout_below="@id/nama_2"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/gambar_2"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/instagram"/>

                <TextView
                    android:id="@+id/ig_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="-----------"
                    android:fontFamily="@font/dm_sans"
                    android:textColor="@color/black"
                    android:layout_toRightOf="@id/gambar_2"
                    android:layout_marginTop="10dp"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>

            <View
                android:id="@+id/divider_2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#000000"
                android:layout_below="@id/dua"
                app:layout_constraintVertical_bias="0.0" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tiga3"
            android:layout_below="@id/dua2">
            <TextView
                android:id="@+id/nama_3"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Denta Kurnia Ramadhani"
                android:fontFamily="@font/dm_sans"
                android:textColor="@color/black"
                android:layout_centerHorizontal="true"/>
            <RelativeLayout
                android:id="@+id/tiga"
                android:layout_width="wrap_content"
                android:layout_below="@id/nama_3"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/gambar_3"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/instagram"/>

                <TextView
                    android:id="@+id/ig_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="-----------"
                    android:fontFamily="@font/dm_sans"
                    android:textColor="@color/black"
                    android:layout_toRightOf="@id/gambar_3"
                    android:layout_marginTop="10dp"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>

            <View
                android:id="@+id/divider_3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#000000"
                android:layout_below="@id/tiga"
                app:layout_constraintVertical_bias="0.0" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/empat4"
            android:layout_below="@id/tiga3">
            <TextView
                android:id="@+id/nama_4"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Alwi Meilino"
                android:fontFamily="@font/dm_sans"
                android:textColor="@color/black"
                android:layout_centerHorizontal="true"/>
            <RelativeLayout
                android:id="@+id/empat"
                android:layout_width="wrap_content"
                android:layout_below="@id/nama_4"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/gambar_4"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/instagram"/>

                <TextView
                    android:id="@+id/ig_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="-----------"
                    android:fontFamily="@font/dm_sans"
                    android:textColor="@color/black"
                    android:layout_toRightOf="@id/gambar_4"
                    android:layout_marginTop="10dp"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>

            <View
                android:id="@+id/divider_4"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#000000"
                android:layout_below="@id/empat"
                app:layout_constraintVertical_bias="0.0" />
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/lima5"
            android:layout_below="@id/empat4"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp">
            <TextView
                android:id="@+id/nama_5"
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Reza Faaldiansyah"
                android:fontFamily="@font/dm_sans"
                android:textColor="@color/black"
                android:layout_centerHorizontal="true"/>
            <RelativeLayout
                android:id="@+id/lima"
                android:layout_width="wrap_content"
                android:layout_below="@id/nama_5"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true">
                <ImageView
                    android:id="@+id/gambar_5"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/instagram"/>

                <TextView
                    android:id="@+id/ig_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="-----------"
                    android:fontFamily="@font/dm_sans"
                    android:textColor="@color/black"
                    android:layout_toRightOf="@id/gambar_5"
                    android:layout_marginTop="10dp"
                    android:layout_centerVertical="true"/>
            </RelativeLayout>

        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>